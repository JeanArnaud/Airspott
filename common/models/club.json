{
  "name": "Club",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number",
      "required": true
    },
    "capacity": {
      "type": "number",
      "required": true
    },
    "openingHours": {
      "type": "object",
      "required": false
    },
    "saleUnit": {
      "type": "string",
      "required": true
    },
    "responsibleName": {
      "type": "string",
      "required": true
    },
    "responsibleTel": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "address": {
      "type": "embedsOne",
      "model": "Address",
      "foreignKey": ""
    },
    "clubSettings": {
      "type": "hasMany",
      "model": "ClubSetting",
      "foreignKey": ""
    },
    "media": {
      "type": "hasMany",
      "model": "Media",
      "foreignKey": ""
    },
    "offers": {
      "type": "hasMany",
      "model": "Offer",
      "foreignKey": ""
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "tax": {
      "type": "belongsTo",
      "model": "TAX",
      "foreignKey": ""
    },
    "planningEntries": {
      "type": "hasMany",
      "model": "PlanningEntry",
      "foreignKey": ""
    },
    "baseCurrency": {
      "type": "hasOne",
      "model": "Currency",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
